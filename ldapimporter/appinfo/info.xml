<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>ldapimporter</id>
    <name>Ldap Importer</name>
    <summary>LDAP importer</summary>
    <description><![CDATA[Synchronize users and group from ldap to nextcloud databases]]></description>
    <version>0.1.0</version>
    <licence>agpl</licence>
    <author>RECIA</author>
    <namespace>LdapImporter</namespace>
    <category>integration</category>
    <bugs>https://github.com/GIP-RECIA/nextcloud-plugins</bugs>
    <dependencies>
        <nextcloud min-version="31" max-version="32" />
    </dependencies>
    <commands>
        <command>OCA\LdapImporter\Command\CreateUser</command>
        <command>OCA\LdapImporter\Command\UpdateUser</command>
        <command>OCA\LdapImporter\Command\ImportUsersAd</command>
        <command>OCA\LdapImporter\Command\DisableDeletedUser</command>
        <command>OCA\LdapImporter\Command\RemoveDisabledUser</command>
    </commands>
    <settings>
        <admin>OCA\LdapImporter\Settings\Admin</admin>
        <admin-section>OCA\LdapImporter\Settings\Sections\Admin</admin-section>
    </settings>
</info>